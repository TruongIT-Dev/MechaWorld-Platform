{
    "rewrites": [
        {
            "source": "/v1/auctions/(.*)/stream",
            "destination": "/v1/auctions/$1/stream"
        },
        {
            "source": "/v1/(.*)",
            "destination": "/v1/$1"
        },
        {
            "source": "/((?!v1).*)",
            "destination": "/index.html"
        }
    ]
}